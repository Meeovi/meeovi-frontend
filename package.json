{
  "name": "mmeeovi",
  "type": "module",
  "version": "0.0.7",
  "description": "This is a storefront for Magento with Prisma and Algolia.",
  "author": "Sebastian Hilton (https://www.sebastianhilton.com)",
  "homepage": "https://template.meeovi.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meeovi/meeovi-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/Meeovi/meeovi-frontend/issues"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "m-commit": "git add . && git commit -m \"ready for update\"",
    "m-update": "git pull \"https://github.com/Meeovi/meeovi-frontend\"",
    "build": "prisma generate && nuxt build",
    "build-sw": "SW=true nuxi build",
    "dev": "nuxt dev --host 127.0.0.1 --port 3011",
    "dev-sw": "SW=true nuxi dev",
    "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "analyze": "nuxi analyze",
    "postinstall": "nuxt prepare && prisma generate",
    "all": "npm-run-all --parallel dev start",
    "lint": "eslint .",
    "typecheck": "nuxt prepare && nuxi typecheck",
    "gqlg": "npx gqlg --schemaFilePath graphql/schema.graphql --destDirPath graphql/queries-mutations_subscriptions --depthLimit 5",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@atproto/api": "^0.14.19",
    "@atproto/common": "^0.4.10",
    "@atproto/identity": "^0.4.7",
    "@atproto/oauth-client-node": "^0.2.14",
    "@atproto/sync": "^0.1.20",
    "@atproto/syntax": "^0.4.0",
    "@atproto/xrpc-server": "^0.7.15",
    "@directus/sdk": "^17.0.2",
    "@directus/types": "^12.1.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "@mdi/font": "^7.4.47",
    "@meilisearch/instant-meilisearch": "^0.25.0",
    "@nuxtjs/i18n": "^9.3.1",
    "@nuxtjs/leaflet": "^1.2.4",
    "@nuxtjs/supabase": "^1.5.0",
    "@pinia/nuxt": "^0.5.4",
    "@prisma/nuxt": "^0.0.35",
    "@rocket.chat/sdk": "^0.2.9-2",
    "@supabase/supabase-js": "^2.49.1",
    "@types/leaflet": "^1.9.14",
    "@vue-storefront/sdk": "^3.4.1",
    "@vue-storefront/storefront-boilerplate-sdk": "^0.2.0",
    "@vue/apollo-composable": "^4.2.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^12.7.0",
    "@vueuse/nuxt": "^13.0.0",
    "@vueuse/shared": "^12.5.0",
    "dotenv": "latest",
    "dotenv-cli": "^7.3.0",
    "instantsearch.css": "^8.5.1",
    "jquery": "^3.7.1",
    "meilisearch": "^0.49.0",
    "mvideoplayer": "^1.0.1",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-security": "^2.1.5",
    "pinia": "^2.3.1",
    "resend": "^4.2.0",
    "sass": "^1.85.1",
    "vee-validate": "^4.15.0",
    "videojs": "^1.0.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-instantsearch": "^4.20.6",
    "vuetify": "^3.6.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.0",
    "@graphql-codegen/typescript-document-nodes": "^4.0.15",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@graphql-codegen/typescript-vue-urql": "^3.2.0",
    "@nuxt/image": "^1.9.0",
    "@nuxtjs/tailwindcss": "6.8.0",
    "@nuxtjs/turnstile": "^0.7.1",
    "@prisma/client": "^6.5.0",
    "@rollup/plugin-graphql": "^2.0.5",
    "@storefront-ui/nuxt": "^2.5.2",
    "@stripe/stripe-js": "2.1.7",
    "@tinymce/tinymce-vue": "latest",
    "@twicpics/components": "^0.32.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^20.11.13",
    "@vue-storefront/unified-data-model": "^3.0.0",
    "cors": "^2.8.5",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "nuxt": "^3.16.0",
    "prisma": "^6.5.0",
    "stripe": "^13.8.0",
    "typescript": "^5.8.2"
  },
  "workspaces": [
    "layers/*"
  ]
}
